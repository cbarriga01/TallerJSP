<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="orm.Contacto" table="contacto" lazy="false">
		<id name="uid" column="uid" type="integer" unsaved-value="0">
			<generator class="sequence">
				<param name="sequence">seq_contacto</param>
			</generator>
		</id>
		<property name="nombre" column="nombre" type="string" length="100" not-null="false" lazy="false">
			<meta attribute="field-description">
				Nombre
			</meta>
		</property>
		<property name="apellido" column="apellido" type="string" length="100" not-null="false" lazy="false">
			<meta attribute="field-description">
				Apellido
			</meta>
		</property>
		<property name="mail" column="mail" type="string" length="50" not-null="false" lazy="false">
			<meta attribute="field-description">
				Mail
			</meta>
		</property>
		<property name="telefono" column="telefono" type="string" length="20" not-null="false" lazy="false">
			<meta attribute="field-description">
				Telefono
			</meta>
		</property>
		<property name="pais" column="pais" type="string" length="20" not-null="false" lazy="false"/>
		<property name="region" column="region" type="string" length="20" not-null="false" lazy="false"/>
		<property name="ciudad" column="ciudad" type="string" length="20" not-null="false" lazy="false"/>
		<many-to-one name="empresa" cascade="lock" column="Empresaid" class="orm.Empresa" not-null="true" lazy="no-proxy" access="field">
		</many-to-one>
	</class>
</hibernate-mapping>
